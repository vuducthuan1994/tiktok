{{>client/footer}}
<script type="text/javascript"> activePage = 'Top Videos';</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.0.7/js/swiper.min.js"></script>
<script type="text/javascript"> $(document).ready(function () {
        $(".searchresultrightdiv").css("max-height", $(".searchresultleft").height()); parseEmoji();
    });
    $("#img_6822810817029803269").hover(function () { $(".userinpic").css("display", "block"); },
        function () { $(".userinpic").css("display", "none"); }); $(".userinpic").hover(function () { $(".userinpic").css("display", "block"); },
            function () { $(".userinpic").css("display", "none"); });
    var swiper = new Swiper("#stroiesGallery", {
        pagination: "#stroiesGallery .swiper-pagination",
        preloadImages: false, lazy: true,
        watchSlidesVisibility: true,
        slidesPerView: 3, paginationClickable: true,
        spaceBetween: 5,
        breakpoints: { 1024: { slidesPerView: 3, spaceBetween: 20 }, 768: { slidesPerView: 2, spaceBetween: 20, }, 420: { slidesPerView: 1, spaceBetween: 10, } },

        navigation: {
            nextEl: "#stroiesGallery .swiper-button-next",
            prevEl: "#stroiesGallery .swiper-button-prev"
        }
    });
</script>