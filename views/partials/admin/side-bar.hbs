            $('ul > li > a[href="' + pathname + '"]').parent().parent().prev('.dropdown-toggle').attr("data-active", "true");
            $('ul > li > a[href="' + pathname + '"]').parent().parent().prev('.dropdown-toggle').attr("aria-expanded", "true");
        }

    });

</script>